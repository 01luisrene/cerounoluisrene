<!DOCTYPE html>
<html>
<head>
    {{> "head"}}
</head>
<body class="{{body_class}} nav-closed">
{{!-- The blog navigation links --}}
{{navigation}}
<div class="site-wrapper">
{{#post}}
<main class="content" role="main">
<div class="wrapper inner-post">
    <article class="{{post_class}}">
        <header class="page-header">
            <h1 class="post-title">{{title}}</h1>
            {{#if image}}
                <img src="{{image}}" alt="imagen - {{title}}">
            {{/if}}
        </header>

        <section class="post-content">
            <hr/>
            {{content}}
        </section>
    </article>
    {{> "aside"}}
</div>
</main>
{{/post}}
{{!-- The tiny footer at the very bottom --}}
{{> "footer"}}
</div>
{{> "scripts"}}
<script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyAIokqr9g-NE7L0CZFH3sx9DHX4VgDkuqw"></script>
<!-- Llamo a mi script map-->
<script type="text/javascript" src="{{asset "js/geo.html5.min.js"}}"></script>

</body>
</html>